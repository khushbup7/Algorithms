package algorithms.linkedlists;

import algorithms.Shuffle;

public class LinkedList {
	
	public Node createLinkedList() {
		int[] arr = new int[10];
		for(int i=0;i<10;i++) {
			arr[i] = i;
		}
		
		Shuffle.shuffle(arr);
		Node temp = new Node();
		Node dummy = temp;
		for(int i=0; i< arr.length; i++) {
			Node n1 = new Node();
			temp.next = n1;
			temp = n1;
		}
		return dummy.next;
		
	}
}
